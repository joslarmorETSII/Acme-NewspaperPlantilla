		<bean class="converters.StringToFolderConverter"/>
		<bean class="converters.StringToSubscribeNewsPaperConverter"/>
		<bean class="converters.StringToSubscribeVolumeConverter"/>
		<bean class="converters.SubscribeNewsPaperToStringConverter"/>
		<bean class="converters.SubscribeVolumeToStringConverter"/>


